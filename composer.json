{
  "name": "pollin14/laravel-curp-validation",
  "description": "Your Package Description here",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "curp",
    "mexico",
    "valiations",
    "laravel"
  ],
  "authors": [
    {
      "name": "Victor Aguilar",
      "email": "victor.aguilar@ciencias.unam.mx"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {},
  "require-dev": {
    "symfony/thanks": "^1.0",
    "phpunit/phpunit": "^7.4@dev",
    "mockery/mockery": "^1.0@dev",
    "orchestra/testbench": "^3.8@dev",
    "orchestra/database": "^3.8@dev",
    "illuminate/support": "^5.8@dev",
    "fzaninotto/faker": "^1.9@dev"
  },
  "autoload": {
    "psr-4": {
      "Pollin14\\LaravelCurpValidation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollin14\\LaravelCurpValidation\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pollin14\\LaravelCurpValidation\\LaravelCurpValidationServiceProvider"
      ],
      "aliases": {
        "Pollin14": "Pollin14\\LaravelCurpValidation\\LaravelCurpValidationFacade"
      }
    }
  }
}
